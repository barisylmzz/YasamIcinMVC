@model List<YasamIcin.UI.MVC.Areas.Admin.Models.OnayDTO>
@{
    ViewBag.Title = "Index";
}

<div class="col-lg-12">
    <table class="table table-hover">
        <thead>
            <tr>
                <th>Kayıt Forumları</th>
                <th>Kayıt Tarihi</th>
            </tr>
        </thead>
        <tbody>
            @foreach (YasamIcin.UI.MVC.Areas.Admin.Models.OnayDTO item in Model)
            {
                <tr><td>
                @Html.ActionLink(item.Isim+" "+item.Soyisim, "AdminOnayla", new RouteValueDictionary(new { id = item.ID }))
            </td>

                    <td>@item.OlusturulmaTarihi</td>
                </tr>
            }
        </tbody>
    </table>
</div>


